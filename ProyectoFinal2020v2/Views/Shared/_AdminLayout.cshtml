@using Microsoft.AspNetCore.Http;

@{
    ViewData["Title"] = "Administrador";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title> @ViewData["Title"] - ProyectoFinal2020</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    @*<link rel="stylesheet" type="text/css" href="~/DataTables/datatables.min.css"  />*@
    <link rel="stylesheet" type="text/css" href="~/DataTables/DataTables-1.10.22/css/dataTables.bootstrap4.min.css" />
    @*<link rel="stylesheet" href="~/css/sweetalert2.min.css" />*@
    <link rel="stylesheet" href="~/css/NavBarMenuGymGarderia.css" />
    <link rel="stylesheet" href="~/css/Recarga.css" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
    @RenderSection("Style", false)
    <!--Referencias para la creacion del carusel-->
    @* <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">*@
    <!-- Minified JS library -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
    <!-- Compiled and minified Bootstrap JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>

</head>

<body>
    <div id="mySidebar" class="sidebar">
        <a href="javascript:void(0)" class="closebtn" onclick="closeNav()">&times;</a>
        <a href="/Home/Index"><i class="fa fa-fw fa-home"></i> Home</a>
        <a href="#"><i class="fas fa-fire-alt"></i> About</a>
        <a href="#"><i class="fa fa-fw fa-wrench"></i> Services</a>
        <a href="#"><i class="fa fa-fw fa-envelope"></i> Contact</a>
        <button class="dropdown-btn">
            <i class="fa fa-fw fa-user"></i>  Cliente
            <i class="fa fa-caret-down"></i>
        </button>
        <div class="dropdown-container">
            <a href="/Clientes/Index">Ver cliente</a>
            <a href="/Clientes/create">Agregar cliente</a>
        </div>

        <button class="dropdown-btn">
            <i class="fas fa-id-badge"></i>
            Empleado
            <i class="fa fa-caret-down"></i>
        </button>
        <div class="dropdown-container">
            <a href="/Empleados/Index">Ver empleados</a>
            <a href="/Empleados/create">Agregar empleados</a>
            @*<a href="#">Link 3</a>*@
        </div>
        <a href="#">Contact2</a>
    </div>
    <header>

        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                @*<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Inicio</a>*@
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Usuarios" asp-action="Index"><i class="fas fa-users fa-2x"></i>Usuarios</a>
                        </li>

                        <li>
                            <div class="dpdownMenu">
                                <button type="button" class="dropbtnMenu"><i class="fas fa-dumbbell"></i> Gym <i class="fa fa-caret-down"></i></button>
                                <div class="dpdownMenu-content">
                                    <div class="header">
                                        <h2><i class="fas fa-dumbbell"></i> Menu Gym</h2>
                                    </div>
                                    <div class="rowMenu">
                                        <div class="columnMenu">
                                            <h4>Gestion de actividad</h4>
                                            <a href="/Actividades/Index">Actividades</a>
                                            <a href="#">Alta de actividad</a>
                                            <a href="#"></a>
                                        </div>
                                        <div class="columnMenu">
                                            <h4> Gestion clase</h4>
                                            <a href="/clasesGym/Index">Mostrar clases</a>
                                            <a href="/clasesGym/create">Agregar clase</a>
                                            <a href="#">Link 3</a>
                                        </div>
                                        <div class="columnMenu">
                                            <h4>Nueva gestion</h4>
                                            <a href="/Salas/Create">Alta Sala</a>
                                            <a href="#">Alta Casillero</a>
                                            <a href="#">Alta Producto</a>
                                            <a href="#">Alta Tarida</a>
                                        </div>
                                        <div class="columnMenu">
                                            <h4>Consulta</h4>
                                            <a href="/Salas/Index">Sala</a>
                                            <a href="#">Casillero</a>
                                            <a href="#">Producto</a>
                                            <a href="#">Tarifa</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="dpdownMenu">
                                <button class="dropbtnMenu"><i class="fas fa-baby-carriage"></i> Guardería  <i class="fa fa-caret-down"></i></button>
                                <div class="dpdownMenu-content">
                                    <div class="header">
                                        <h2><i class="fas fa-baby"></i> Menu Guardería</h2>
                                    </div>
                                    <div class="rowMenu">
                                        <div class="columnMenu">
                                            <h3>Inscripción a guardería</h3>
                                            <a href="#">Matrícula</a>
                                            <a href="#">Alta clase</a>
                                            <a href="#">Alta de padre</a>
                                            <a href="#">Alta de bebe</a>

                                        </div>
                                        <div class="columnMenu">
                                            <h3>Consulta</h3>
                                            <a href="#">Matrícula</a>
                                            <a href="#">Clases</a>
                                            <a href="#">Padre</a>@*Cliente(Controler)/(padre)action->Index*@
                                            <a href="#">Bebé</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </li>
                        <li>
                            <div class="dropRecardown">
                                <button class="dropRecarbtn">Recarga  <i class="fa fa-caret-down"></i></button>
                                <div class="dropRecardown-content">
                                    <a href="#">Monedero virtual</a>
                                    <a href="#">Renovación</a>
                                    <a href="#">Compra</a>
                                </div>
                            </div>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                <ul class="navbar-nav flex-grow-1">

                    <li class="nav-item">
                        <a class="nav-link text-dark"><i class="fas fa-user"></i>Hola @Context.Session.GetString("NombreUsuario") !</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Login" asp-action="Logout"><i class="fas fa-door-open"></i>Cerrar Sesión</a>
                    </li>

                </ul>
            </div>
            <div id="main">
                <a class="openbtn" onclick="openNav()">&#9776</a>
            </div>
        </nav>
    </header>



    @*<div class="container1">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>*@
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>


    <footer class="border-top modal-footer text-muted">
        <div class="container">
            &copy; 2020 - ProyectoFinal2020 - @*<a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>*@
        </div>
    </footer>


    <script src="https://kit.fontawesome.com/2775cf0011.js" crossorigin="anonymous"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @*<script src="~/js/datatables.min.js" asp-append-version="true"></script>*@
    <script src="~/js/sweetalert2.all.min.js" asp-append-version="true"></script>
    @*<script src="~/js/jquery.editable.min.js" asp-append-version="true"></script>*@

    @*<script src="~/js/sweetalert2.min.js" asp-append-version="true"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@10"></script>
    @*DATATABLE BOOTSTRAP Y EXPORT*@
    <script src="~/DataTables/JSZip-2.5.0/jszip.min.js"></script>
    <script src="~/DataTables/pdfmake-0.1.36/pdfmake.min.js"></script>
    <script src="~/DataTables/pdfmake-0.1.36/vfs_fonts.js"></script>
    <script src="~/DataTables/Buttons-1.6.5/js/buttons.html5.min.js"></script>
    <script src="~/DataTables/Buttons-1.6.5/js/dataTables.buttons.min.js"></script>

    <script type="text/javascript" src="~/DataTables/datatables.min.js"></script>

    @RenderSection("Scripts", required: false)
